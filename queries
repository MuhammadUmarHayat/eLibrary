SELECT 
    books.bookid,books.title
FROM
    books
WHERE
    bookid IN (SELECT 
            bookid
        FROM
            userbooks
        WHERE
            bookid = '3');
			//////////////////////////////////
			
			SELECT books.bookid,books.title,books.photo,books.publisher,userbooks.userid,userbooks.bookid FROM books INNER JOIN userbooks ON books.bookid=userbooks.bookid
